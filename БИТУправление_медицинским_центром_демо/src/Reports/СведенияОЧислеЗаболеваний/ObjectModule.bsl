#Область ОбработчикиСобытий

Процедура ПриКомпоновкеРезультата(ДокументРезультат, ДанныеРасшифровки, СтандартнаяОбработка)
	
	СтандартнаяОбработка = Ложь;
		
	Если Ложь Тогда
		ДокументРезультат = Новый ТабличныйДокумент;
	КонецЕсли;
	
	
	КомпоновщикМакета = Новый КомпоновщикМакетаКомпоновкиДанных;
	Настройки = КомпоновщикНастроек.ПолучитьНастройки();
	МакетКомпоновки = КомпоновщикМакета.Выполнить(СхемаКомпоновкиДанных, Настройки,,, Тип("ГенераторМакетаКомпоновкиДанныхДляКоллекцииЗначений"));
	
	ПроцессорКомпоновкиДанных = Новый ПроцессорКомпоновкиДанных;
	ПроцессорКомпоновкиДанных.Инициализировать(МакетКомпоновки);	

	ДеревоРезультат = Новый ДеревоЗначений;
	ПроцессорВывода = Новый ПроцессорВыводаРезультатаКомпоновкиДанныхВКоллекциюЗначений;
	ПроцессорВывода.УстановитьОбъект(ДеревоРезультат);
	ПроцессорВывода.Вывести(ПроцессорКомпоновкиДанных, Истина);
	
	СтруктураКолонок = Новый Структура;
	Для Каждого Колонка Из ДеревоРезультат.Колонки Цикл
		Если Колонка.ТипЗначения.СодержитТип(Тип("Число")) Тогда
			СтруктураКолонок.Вставить(Колонка.Имя, 0);
		КонецЕсли;
	КонецЦикла;
	
	Макет = ПолучитьМакет("Макет");
	ОбластьШапка = Макет.ПолучитьОбласть("Шапка");
	Период = Настройки.ПараметрыДанных.Элементы.Найти("Период");
	Если Период <> Неопределено Тогда
		ОбластьШапка.Параметры.Год = Формат(Период.Значение.ДатаОкончания, "ДФ=yyyy");
	КонецЕсли;
	ДокументРезультат.Вывести(ОбластьШапка);
	
	ОбластьДанныеОрганизации = Макет.ПолучитьОбласть("ДанныеОрганизации");
	ДанныеОрганизации = ПроцедурыСпециализацииПоставки.ПолучитьРеквизитыОрганизации(ПланыОбмена.Полный.ЭтотУзел());
	ОбластьДанныеОрганизации.Параметры.НаименованиеОрганизации = ДанныеОрганизации.НазваниеОрганизации;
	ОбластьДанныеОрганизации.Параметры.ОКПО = ДанныеОрганизации.ОКПО;
	ОбластьДанныеОрганизации.Параметры.ПочтовыйАдресОрганизации = ДанныеОрганизации.Адрес;		
	ДокументРезультат.Вывести(ОбластьДанныеОрганизации);
	ДокументРезультат.ВывестиГоризонтальныйРазделительСтраниц();
	
	// Дети (до 14)
	ОбластьШапкаТаблицы = Макет.ПолучитьОбласть("ШапкаТаблицы1");
	ОбластьШапкаТаблицы.Параметры.Номер = 1;
	ОбластьШапкаТаблицы.Параметры.ЗаголовокТаблицы = "Дети (до 14 лет включительно)";
	ДокументРезультат.Вывести(ОбластьШапкаТаблицы);
	
	Для Каждого Параметр Из ПолучитьПараметрыСтраницы("До14") Цикл
		СтрокаТаблицы = Макет.ПолучитьОбласть("СтрокаТаблицы1До14");
		СтрокаТаблицы.Параметры.Заполнить(Параметр);
		ОбластьНаим = СтрокаТаблицы.Область("ТекстПунктаТаблицы1До14");
		Если Параметр.Свойство("Имя") Тогда
			РезПоиска = НайтиПоДиапазону(ДеревоРезультат, Параметр.Имя, СтруктураКолонок);			
			СтрокаТаблицы.Параметры.До14 = РезПоиска.До14;
			СтрокаТаблицы.Параметры.До14Впервые = РезПоиска.До14Впервые;	
			ОбластьНаим.Отступ = СтрЧислоВхождений(Параметр.НомерСтроки, ".") * 2;
			ВсяОбласть = СтрокаТаблицы.Область("ВсяОбластьСтрокиТаблицы1До14");
			ВсяОбласть.Шрифт = Новый Шрифт(ВсяОбласть.Шрифт, , 9, Параметр.Жирный);
		Иначе
			ОбластьНаим.Отступ = Параметр.Отступ;	
		КонецЕсли;
		ДокументРезультат.Вывести(СтрокаТаблицы);
	КонецЦикла;
	
	ОбластьШапкаТаблицы = Макет.ПолучитьОбласть("ШапкаТаблицы2");
	ОбластьШапкаТаблицы.Параметры.Номер = 1;
	ДокументРезультат.Вывести(ОбластьШапкаТаблицы);
	
	Для Каждого Параметр Из ПолучитьПараметрыСтраницы("Факторы") Цикл
		СтрокаТаблицы = Макет.ПолучитьОбласть("СтрокаТаблицы2До14");
		СтрокаТаблицы.Параметры.Заполнить(Параметр);
		ОбластьНаим = СтрокаТаблицы.Область("ТекстПунктаТаблицы2До14");
		Если Параметр.Свойство("Имя") Тогда
			РезПоиска = НайтиПоДиапазону(ДеревоРезультат, Параметр.Имя, СтруктураКолонок);
			СтрокаТаблицы.Параметры.До14 = РезПоиска.До14;	
			ОбластьНаим.Отступ = СтрЧислоВхождений(Параметр.НомерСтроки, ".") * 2;
			ВсяОбласть = СтрокаТаблицы.Область("ВсяОбластьСтрокиТаблицы2До14");
			ВсяОбласть.Шрифт = Новый Шрифт(ВсяОбласть.Шрифт, , 9, Параметр.Жирный);
		Иначе
			ОбластьНаим.Отступ = Параметр.Отступ;	
		КонецЕсли;
		ДокументРезультат.Вывести(СтрокаТаблицы);
	КонецЦикла;
	ДокументРезультат.ВывестиГоризонтальныйРазделительСтраниц();
	
	// Подростки (15-17)
	ОбластьШапкаТаблицы = Макет.ПолучитьОбласть("ШапкаТаблицы1От15До17");
	ОбластьШапкаТаблицы.Параметры.Номер = 2;

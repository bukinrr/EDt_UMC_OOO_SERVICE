#КонецОбласти

#Область СлужебныеПроцедурыИФункции

Функция НайтиПоКодуВДеревеДиагнозов(ДеревоДиагнозов, Код, Поле = Неопределено)
	
	Для Каждого Строка Из ДеревоДиагнозов.Строки Цикл
		
		Если Строка.Ссылка.Код = Код Тогда
			Возврат ?(Поле = Неопределено, Строка, Строка[Поле]);
		Иначе
			ПоискВВетви = НайтиПоКодуВДеревеДиагнозов(Строка, Код, Поле);
			Если ПоискВВетви <> Неопределено И ПоискВВетви <> 0 Тогда
				Возврат ?(Поле = Неопределено, ПоискВВетви, ПоискВВетви[Поле]);
			КонецЕсли;
		КонецЕсли;	
	КонецЦикла;
	
	Если Поле = Неопределено Или Поле = "Ссылка" Тогда
		Возврат Неопределено;
	Иначе
		Возврат 0;
	КонецЕсли;
	
КонецФункции

Функция НайтиПоДиапазону(ДеревоДиагнозов, Диапазон, Знач СтруктураСвертка)
	
	Для Каждого элСтруктуры Из СтруктураСвертка Цикл
		СтруктураСвертка[элСтруктуры.Ключ] = 0;
	КонецЦикла;
	
	// Разбиваем диапазон, если их несколько
	мсДиапазоны = СтроковыеФункцииКлиентСервер.РазложитьСтрокуВМассивПодстрок(Диапазон, "___");
	Для Каждого элДиапазон Из мсДиапазоны Цикл
		элДиапазон = СтрЗаменить(СтрЗаменить(элДиапазон,"__","."),"_","-");
		// Получаем начало и конец диапазона
		НачКон = СтроковыеФункцииКлиентСервер.РазложитьСтрокуВМассивПодстрок(элДиапазон,"-");
		Если НачКон.Количество() = 0 Тогда
			Продолжить;
		ИначеЕсли НачКон.Количество() = 1 Тогда
			НачКон.Добавить(НачКон[0]);
		КонецЕсли;
		ПросуммироватьДиагнозыВДиапазоне(СтруктураСвертка, ДеревоДиагнозов, элДиапазон, НачКон);	
	КонецЦикла;
	
	Возврат СтруктураСвертка;
	
КонецФункции

Процедура ПросуммироватьДиагнозыВДиапазоне(Результат, ДеревоДиагнозов, элДиапазон, НачКон)
	
	Для Каждого Строка Из ДеревоДиагнозов.Строки Цикл
		
		НаимДиапазон = "";
		ДиапазонЭлемента = Новый Массив;
		
		Если Не ЗначениеЗаполнено(Строка.Диагноз) Тогда
			Продолжить;
		КонецЕсли;
		
		КодДиагноза = Строка.Диагноз.КодДиагноза;
		ДиапазонЭлемента = СтроковыеФункцииКлиентСервер.РазложитьСтрокуВМассивПодстрок(КодДиагноза, "-");
		Если ДиапазонЭлемента.Количество() = 1 Тогда // Если не диапазон - расширяем до диапазона из двух одинаковых
			ДиапазонЭлемента.Добавить(ДиапазонЭлемента[0]);
		КонецЕсли;		
		
		ВходВДиапазон = ВходитВДиапазон(НачКон, ДиапазонЭлемента);
		Если ВходВДиапазон = 0 Тогда
			Для Каждого кзРез Из Результат Цикл
				Результат.Вставить(кзРез.Ключ, кзРез.Значение + Строка[кзРез.Ключ]);
			КонецЦикла;
		ИначеЕсли ВходВДиапазон = 2 Тогда
			Продолжить;
		ИначеЕсли ВходВДиапазон = 1 И Строка.Диагноз.ЭтоГруппа Тогда
			ПросуммироватьДиагнозыВДиапазоне(Результат, Строка, элДиапазон, НачКон);
		КонецЕсли;		

		Если НачКон[1] = ДиапазонЭлемента[1] Тогда // Конец диапазона нашли
			Возврат;
		КонецЕсли;
	
	КонецЦикла;
	
КонецПроцедуры

// Проверяет пересечение двух диапазонов
// 0 - элемент полностью в диапазоне
// 1 - диапазон частично или полностью в элементе
// 2 - не пересекаются.
Функция ВходитВДиапазон(Диапазон, ДиапазонЭлемента)
	
	Если СравнитьСтроки(ДиапазонЭлемента[0], Диапазон[1]) <= 0 И СравнитьСтроки(ДиапазонЭлемента[1], Диапазон[0]) >= 0 Тогда
		Если СравнитьСтроки(Диапазон[0], ДиапазонЭлемента[0]) <= 0 И СравнитьСтроки(Диапазон[1], ДиапазонЭлемента[1]) >= 0 И
			(СтрДлина(Диапазон[0]) = СтрДлина(ДиапазонЭлемента[0]) Или СтрДлина(Диапазон[1]) = СтрДлина(ДиапазонЭлемента[1])) 
		Тогда // Элемент полностью в диапазоне
			Возврат 0;
		Иначе
			Возврат 1;
		КонецЕсли;	
	Иначе
		Возврат 2;
	КонецЕсли;
	
КонецФункции

Функция СравнитьСтроки(Знач Стр1, Знач Стр2)
	
	Если Стр1 = Стр2 Тогда
		Возврат 0;
	Иначе
		Стр1ОдинДлины = Стр1;
		Стр2ОдинДлины = Стр2;

		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	Клиенты.ДатаРождения,
	Клиенты.Пол,
	Клиенты.Ссылка КАК Клиент,
	Клиенты.Наименование,
	ДЕНЬ(Клиенты.ДатаРождения) КАК ДеньРождения,
	МЕСЯЦ(Клиенты.ДатаРождения) КАК МесяцРождения,
	ГОД(Клиенты.ДатаРождения) КАК ГодРождения,
	ВЫБОР
		КОГДА ДЕНЬГОДА(Клиенты.ДатаРождения) &lt;= ДЕНЬГОДА(&amp;ТекущаяДата)
			ТОГДА РАЗНОСТЬДАТ(Клиенты.ДатаРождения, &amp;ТекущаяДата, ГОД)
		ИНАЧЕ РАЗНОСТЬДАТ(Клиенты.ДатаРождения, ДОБАВИТЬКДАТЕ(&amp;ТекущаяДата, ГОД, -1), ГОД)
	КОНЕЦ КАК Возраст,
	Клиенты.ПометкаУдаления,
	Клиенты.Архив
{ВЫБРАТЬ
	Клиент.*}
ИЗ
	Справочник.Клиенты КАК Клиенты
ГДЕ
	Клиенты.ЭтоГруппа = ЛОЖЬ
{ГДЕ
	Клиенты.ДатаРождения,
	Клиенты.Пол.*,
	Клиенты.Наименование,
	(ДЕНЬ(Клиенты.ДатаРождения)) КАК День,
	(МЕСЯЦ(Клиенты.ДатаРождения)) КАК Месяц,
	(ГОД(Клиенты.ДатаРождения)) КАК Год,
	Клиенты.Ссылка.* КАК Клиент}</query>
	</dataSet>

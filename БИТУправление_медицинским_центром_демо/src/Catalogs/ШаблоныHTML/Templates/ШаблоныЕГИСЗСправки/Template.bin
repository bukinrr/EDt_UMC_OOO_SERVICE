			<DeletionMark>false</DeletionMark>
			<Parent>00000000-0000-0000-0000-000000000000</Parent>
			<Description>Номер справки</Description>
			<АлгоритмВычисления>	Результат = 0;
	
	ДатаМоментВремени = ДокументОбъект.Дата;
	
	Если ЗначениеЗаполнено(ДокументОбъект.Номенклатура)
		И ЗначениеЗаполнено(ДокументОбъект.Номенклатура.ИндивидуальныйШаблонОсмотра)
	Тогда
		
		Запрос = Новый Запрос;
		Запрос.УстановитьПараметр("Дата", ДатаМоментВремени);
		Запрос.УстановитьПараметр("Параметр", Параметр);
		Запрос.УстановитьПараметр("ИндивидуальныйШаблонОсмотра", ДокументОбъект.Номенклатура.ИндивидуальныйШаблонОсмотра);
		Запрос.Текст = 
		"ВЫБРАТЬ ПЕРВЫЕ 50
		|	ПрохождениеМедосмотра.Ссылка КАК Ссылка
		|ПОМЕСТИТЬ ПМО
		|ИЗ
		|	Документ.ПрохождениеМедосмотра КАК ПрохождениеМедосмотра
		|ГДЕ
		|	ПрохождениеМедосмотра.Дата &lt; &amp;Дата
		|	И ПрохождениеМедосмотра.Номенклатура.ИндивидуальныйШаблонОсмотра = &amp;ИндивидуальныйШаблонОсмотра
		|
		|УПОРЯДОЧИТЬ ПО
		|	ПрохождениеМедосмотра.Дата УБЫВ
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	ЗначенияПараметровHTML.Параметр КАК Параметр,
		|	МАКСИМУМ(ЗначенияПараметровHTML.Значение) КАК Значение
		|ИЗ
		|	Документ.ПрохождениеМедосмотра.ПараметрыПриема КАК ЗначенияПараметровHTML
		|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ПМО КАК ПМО
		|		ПО ЗначенияПараметровHTML.Ссылка = ПМО.Ссылка
		|			И (ЗначенияПараметровHTML.Параметр = &amp;Параметр)
		|
		|СГРУППИРОВАТЬ ПО
		|	ЗначенияПараметровHTML.Параметр";
		
		Выборка = Запрос.Выполнить().Выбрать();
		Если Выборка.Следующий() Тогда
			Попытка
				Результат = Выборка.Значение + 1;
			Исключение КонецПопытки;
		КонецЕсли;
		
	КонецЕсли;</АлгоритмВычисления>

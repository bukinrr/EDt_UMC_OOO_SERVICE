			<АлгоритмВычисления>    Клиент = ДокументОбъект.Клиент;
    Результат = "";
	
	// Возвращает полную форму представления адреса за исключением индекса.
	
	// Получаем фактический адрес. Если его нет, то юридический.
	Запись = РегистрыСведений.КонтактнаяИнформация.СоздатьМенеджерЗаписи();
	Запись.Объект = Клиент;
	Запись.Тип = Перечисления.ТипыКонтактнойИнформации.Адрес;
	Запись.Вид = Справочники.ВидыКонтактнойИнформации.АдресФактический;
	Запись.Прочитать();
	Если Не Запись.Выбран() Тогда
		Запись.Объект = Клиент;
		Запись.Тип = Перечисления.ТипыКонтактнойИнформации.Адрес;
		Запись.Вид = Справочники.ВидыКонтактнойИнформации.АдресЮридический;
		Запись.Прочитать();
	КонецЕсли;
	
	Если Запись.Выбран() Тогда
		// Адрес найден
		Если Не ЗначениеЗаполнено(Запись.ЗначенияПолей) Тогда
			Запись = КонтактнаяИнформацияСервер.ОбновитьКИПодФорматФИАС(Клиент, Запись.Вид);	
		КонецЕсли;
		
		ВидИнформации = Новый Структура("ФорматАдреса, ВключатьИндексВПредставление", "ФИАС", Ложь);
		ВидИнформации.Вставить("ВключатьСтрануВПредставление", Ложь); // Вывод страны в адресе не осуществлять.
		
		АдресXDTO = УправлениеКонтактнойИнформациейСлужебный.КонтактнаяИнформацияИзXML(Запись.ЗначенияПолей);
		АдресXDTO.Состав.Состав.КраткоеПредставление = Ложь;
		СведенияОбАдресе = РаботаСАдресами.СведенияОбАдресе(АдресXDTO);
		
		Представление = СведенияОбАдресе.Представление;
		
		// Убираем отображение индекса
		Если ЗначениеЗаполнено(СведенияОбАдресе.Индекс) 
			И Лев(Представление, СтрДлина(СведенияОбАдресе.Индекс)) = СведенияОбАдресе.Индекс
		Тогда
			Представление = Прав(Представление, СтрДлина(Представление) - СтрДлина(СведенияОбАдресе.Индекс));
			Если Лев(Представление, 1) = "," Тогда
				Представление = Прав(Представление, СтрДлина(Представление) - 1);
			КонецЕсли;
		КонецЕсли;
		
		Результат = СокрЛП(Представление);
	КонецЕсли;</АлгоритмВычисления>

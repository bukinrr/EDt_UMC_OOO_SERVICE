	СтраховыеПолисы.НеДействителен,
	СтраховыеПолисы.ВидПолиса.Контрагент,
	СУММА(ЕСТЬNULL(ОстаткиПоГарантийнымПисьмамОстатки.СуммаОстаток, 0)) КАК Остаток,
	СУММА(ЕСТЬNULL(ОстаткиПоГарантийнымПисьмамНаУслугиОстатки.КоличествоОстаток, 0)) КАК ОстатокУслуг
ИЗ
	Справочник.СтраховыеПолисы КАК СтраховыеПолисы
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			СтраховыеПолисы.Ссылка КАК Ссылка
		ИЗ
			Справочник.СтраховыеПолисы КАК СтраховыеПолисы
		ГДЕ
			СтраховыеПолисы.Владелец = &amp;Владелец) КАК СтраховыеПолисыВынужденный
		ПО СтраховыеПолисы.Ссылка = СтраховыеПолисыВынужденный.Ссылка
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ОстаткиПоГарантийнымПисьмам.Остатки(
				,
				СтраховойПолис.Владелец = &amp;Владелец
					И (ГарантийноеПисьмо.ДатаСрок = ДАТАВРЕМЯ(1, 1, 1, 0, 0, 0)
						ИЛИ ГарантийноеПисьмо.ДатаСрок >= &amp;ТекущаяДата)) КАК ОстаткиПоГарантийнымПисьмамОстатки
		ПО (ОстаткиПоГарантийнымПисьмамОстатки.СтраховойПолис = СтраховыеПолисы.Ссылка)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ОстаткиПоГарантийнымПисьмамНаУслуги.Остатки(
				,
				СтраховойПолис.Владелец = &amp;Владелец

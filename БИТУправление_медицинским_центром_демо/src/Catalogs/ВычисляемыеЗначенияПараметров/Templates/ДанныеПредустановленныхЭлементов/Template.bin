			|&lt;TD style='font-size: 12pt; WORD-WRAP: break-word'&gt;
			|&lt;P align=center&gt;Реакция&lt;/P&gt;&lt;/TD&gt;&lt;/TR&gt;";

ТекстШапкиДобавлен = Ложь;
Текст = "";
Клиент = ДокументОбъект.Клиент;
Запрос=Новый Запрос;
Запрос.УстановитьПараметр("Клиент", Клиент); 
Запрос.Текст ="ВЫБРАТЬ 
              |	РегистрСведенийЛекарственнаяНепереносимость.ДействующееВещество,
              |	РегистрСведенийЛекарственнаяНепереносимость.Реакция

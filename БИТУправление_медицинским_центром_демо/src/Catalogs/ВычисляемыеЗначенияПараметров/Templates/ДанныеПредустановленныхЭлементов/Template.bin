			|&lt;TABLE border=1  cellspacing='0' width = '100%'&gt;
			|&lt;TBODY&gt;
			|&lt;TR&gt;
			|&lt;TD style='font-size: 12pt; WORD-WRAP: break-word; width: 40%'&gt;

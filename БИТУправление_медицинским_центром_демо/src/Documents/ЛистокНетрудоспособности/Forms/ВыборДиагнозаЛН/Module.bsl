#Область ОбработчикиСобытийФормы

&НаСервере
Процедура ПриСозданииНаСервере(Отказ, СтандартнаяОбработка)
	
	Для Каждого Структура Из Параметры.Диагнозы Цикл
		Стр = Диагнозы.Добавить();
		Стр.Код = Структура.Код;
		Стр.Наименование = Структура.Наименование;
		Стр.Травма = Структура.Травма;
	КонецЦикла;
	Значения = Параметры.ЗначенияЗаполнения;
	
КонецПроцедуры

#КонецОбласти

#Область СлужебныеПроцедурыИФункции

&НаКлиенте
Процедура ЗаполнитьПолеДиагноза() 

	Форма = ПолучитьФорму("Документ.ЛистокНетрудоспособности.Форма.ФормаДокумента");
	Значения.Вставить("ДиагнозПоМКБ10", Элементы.Диагнозы.ТекущиеДанные.Наименование);
	Значения.Вставить("ПричинаНетрудоспособности", Элементы.Диагнозы.ТекущиеДанные.Травма);
	ПараметрыФормы = Новый Структура("ЗначенияЗаполнения", Значения);
	ЭтаФорма.Закрыть();
	ОткрытьФорму("Документ.ЛистокНетрудоспособности.Форма.ФормаДокумента", ПараметрыФормы);	
	
КонецПроцедуры

#КонецОбласти